[{"name":"Ancheta organica","gid":52145521,"data":[["Cod","Intrebare"],[100,"Simţiţi că nu vă puteţi păstra atenţia chiar şi într-o discuţie nu prea lungă?"],[101,"Simţiţi că-n ultimul timp vă este greu să vă transferaţi de la o activitate la alta fără a o termina pe prima?"],[102,"Simţiţi că-n ultimul timp nimic nu vă poate strica buna dispoziţie?"],[103,"Vi se schimbă uşor dispoziţia?"],[104,"Reacţionaţi cu indiferenţă la ceea ce se petrece?"],[105,"Vă planificaţi cu greu activităţile pentru zilele următoare?"],[106,"Simţiţi permanent nevoia de a vă mişca sau vă este greu să staţi pe un loc?"],[107,"Simţiti că în ultimul timp s-a intensificat activitatea dumneavoastră sexuală şi simţiţi nevoia de a avea mai mulţi parteneri?"],[108,"Aveţi accese de furie fără un motiv aparent?"],[109,"Simţiţi că în ultimul timp v-a scăzut interesul în activităţi erotice?"],[110,"Simţiţi că în ultimul timp aţi devenit mai mult/puţin sensibil la sunetele din jur?"],[111,"Simţiţi că în ultimul timp vă este mai greu să vă orientaţi în zona (sectorul) unde locuiţi?"],[112,"Simţiţi că în ultimul timp vă este greu să apreciaţi cu ochii închişi, numai prin atingere, materialul din care-i făcut un obiect?"],[113,"Simţiţi că în ultimul timp comiteţi mai multe greşeli gramaticale şi/sau vă vine mai greu să faceţi calcule?"],[114,"Simţiţi că în ultimul timp văzul dumneavoastră suferă schimbări?"],[115,"Simţiţi că în ultimul timp v-a scăzut memoria?"],[116,"Simţiţi că în ultimul timp vi se face foame şi/sau sete mai des sau la scurt timp după ce aţi luat masa?"],[117,"Vă simţiţi foarte vinovat după ce mâncaţi şi simţiţi nevoia să vomitaţi?"],[118,"Simţiţi că în ultimul timp vi se întâmplă să dormiţi mult timp ziua şi să nu dormiţi noaptea?"],[119,"Simţiţi că alţii aşteaptă să greşiţi şi să profite de slăbiciunile dumneavoastră ? "],[120,"Simţiţi că în ultimul timp obosiţi mult mai repede?"],[121,"Simţiţi nevoia să verificaţi acţiunile celor din jur fiindcă nu aveţi încredere în ei?"],[122,"Aveţi accese de frică pe care n-o puteţi lămuri?"]],"sub":false},{"name":"Ancheta specifica","gid":2017715786,"data":[["Code","Question"],[200,"Mă simt criticat de alţii şi de aceea evit să comunic cu persoanele cărora nu ştiu dacă le plac."],[201,"Imediat ce închei o relaţie caut să mă implic în alta deoarece nu suport singurătatea."],[202,"Am impresia că nu reuşesc să fac totul la fel de bine ca alţii şi de aceea am un stil de viaţă mai modest."],[203,"Permit altora să decidă în locul meu în probleme importante."],[204,"Am reguli fixe pentru tot şi întocmesc planuri şi liste minuţioase pentru orice activitate indiferent cât de uşoară ar fi."],[205,"Insist ca toţi să facă totul exact în ordinea şi modul în care fac eu."],[206,"Imi este foarte greu să arunc obiectele mele învechite sau inutile."],[207,"Nu-mi pot defini personalitatea, mă simt permanent altfel."],[208,"Relaţiile mele sentimentale sunt intense dar scurte, fiindcă simt că iubesc şi urăsc partenerul în acelaşi timp."],[209,"Sunt indiferent faţă de problemele altora, în fond ele nu mă privesc."],[210,"Nu mă interesează colaborarea cu alţii sau activităţile de grup."],[211,"Nu mă interesează relaţiile sexuale."],[212,"Simt că partenerii şi colegii mei încearcă să mă înşele."],[213,"Nu fac confidenţe deoarece ştiu că informaţia va fi folosită contra mea şi nu aş putea ierta aşa ceva."],[214,"Ştiu că sunt una dintre cele mai valoroase persoane din ţară."],[215,"Dispoziţia mea \"pluteşte\": ba sunt vesel ba imi vine să plâng."],[216,"Când mă enervez simt nevoia de a lovi în ceva sau cineva."],[217,"Cred că regulile sunt făcute pentru a fi încălcate."],[218,"Sunt o persoană impulsivă, fac multe sub influenţa momentului."],[219,"Cred că merit admiraţia tuturor şi-mi place să fiu în centrul atenţiei."],[220,"Am impresia că nimeni nu mă înţelege."],[221,"O persoană care îmi place poate uşor să-mi schimbe părerea."],[222,"Alţii aşteaptă să greşesc ca să profite de slăbiciunile mele. "],[223,"Trebuie să verific acţiunile celor din jur fiindcă nu am încredere în ei."]],"sub":false},{"name":"Factori ancheta specifica","gid":330986171,"data":[["Calificative","Factor"],["Niciodata",0],["Foarte rar",0.1],["Rar",0.4],["Des",1.1],["Foarte des",1.7],["Permanent",2]],"sub":false},{"name":"Siptoame organice","gid":1405035266,"data":[["Cod","Symptom"],[100,"Hipoprosexie"],[101,"Perseverări psiho-motorii"],[102,"Dispoziţie euforică"],[103,"Labilitate emoţională"],[104,"Limitarea expresiei emoţionale"],[105,"Blocarea capacităţii de planificare"],[106,"Dezinhibiţie comportamentală"],[107,"Hipersexualitate"],[108,"Accese de furie"],[109,"Hiposexualitate"],[110,"Tulburări auditive"],[111,"Tulburări de orientare spaţială"],[112,"Tulburări de sensibilitate"],[113,"Agrafie/acalculie"],[114,"Tulburări vizuale"],[115,"Tulburări de memorie"],[116,"Fluctuaţii de foame/sete"],[117,"Bulimie"],[118,"Tulburarea regimului de somn/veghe"],[119,"Suspiciozitate"],[120,"Astenizare"],[121,"Nevoie de control"],[122,"Anxietate"]],"sub":false},{"name":"Siptoame specifice","gid":198847004,"data":[["Cod","Simptomul"],[200,"Frică de critică"],[201,"Frică de separare"],[202,"Sentiment de inferioritate"],[203,"Subordonare masochistă"],[204,"Preocupare excesivă de detalii"],[205,"Ritualizarea proceselor"],[206,"Rigiditate"],[207,"Instabilitatea imaginii de sine"],[208,"Ambivalenţă"],[209,"Lipsa de empatie"],[210,"Detaşare socială"],[211,"Hiposexualitate"],[212,"Sentiment de nesiguranţă"],[213,"Expectaţie negativă"],[214,"Sentiment de importanţă exagerată"],[215,"Labilitate emoţională"],[216,"Agresivitate"],[217,"Indiferenţă faţă de normele sociale"],[218,"Deficit al controlului impulsurilor"],[219,"Nevoie de atenţie"],[220,"Exagerarea expresiilor emoţionale"],[221,"Sugestibilitate"],[222,"Suspiciozitate"],[223,"Nevoie de control"]],"sub":false},{"name":"Sindroame organice","gid":757940284,"data":[["Code","Syndrome","Weight","Required"],[0,"Sinfromul Lobilor Frontali",0,false],[100,"Hipoprosexie",8,false],[106,"Dezinhibi